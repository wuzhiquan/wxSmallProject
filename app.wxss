}
.btn-area-button {
  width: 100%;
  border-radius: 0;
  border-width: 0;
}
.btn-area button {
  width: 100%;
  border-radius: 0;
  border-width: 0;
}
.btn-area {
  position: fixed;
  bottom: 0;
  width: 100%;
}